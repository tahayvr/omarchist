{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "shell-capability",
	"windows": ["main"],
	"permissions": [
		{
			"identifier": "shell:allow-execute",
			"allow": [
				{
					"name": "nautilus",
					"cmd": "nautilus",
					"args": [
						{
							"validator": ".*"
						}
					]
				}
			]
		}
	]
}
