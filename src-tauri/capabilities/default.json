{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:app:default",
    "opener:default",
    "dialog:default",
    "cli:default",
    "shell:default",
    "allow-get-community-themes",
    "allow-theme-cache-commands"
  ]
}
